
QTabBar::tab { background: #000000 ; border: 4px solid #222; border-bottom-color: #000000 ; border-top-left-radius: 4px; border-top-right-radius: 4px; min-width: 8ex; padding: 4px; color: #888;}
QTabBar::tab:selected, QTabBar::tab:hover { color: yellow;}
QMenu { background-color: #000000; background: #000000; color: #FFFF00;}
QMenu::separator { background: #FFFF00; height: 1px;}
QMenu::item:selected { background-color: #FFFF00; color: #000000 ;}
QMenuBar { background-color: #000000; background: #000000; color: #FFFF00;}
QMenuBar::item { background-color: #000000 ; color: #FFFF00;}
QMenuBar::item:selected { background-color: #FFFF00; color: #000000;}
QToolTip { border: none;}
QTableView { color: #ffff00; border-style: none; selection-background-color: #ffffff; alternate-background-color: #444444; selection-color: #000000 ; border: 0px solid black;}
QHeaderView { color: #ffff00; background-color: transparent; background-image: none;}
QHeaderView::section { color: #ffff00; background-color: transparent;}
